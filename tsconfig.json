{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@icons": ["src/assets/icons/index.ts"]
    },
    "types": ["@nuxt/types", "vite/client"],
  },
  "include": ["./**/*.ts", "./**/*.d.ts", "./**/*.tsx", "./**/*.vue",],
  "exclude": ["node_modules", "dist"]
}
